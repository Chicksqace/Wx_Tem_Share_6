.contanier{
  width: 100%;
  height: 100vh;
 
  display: flex;
  flex-direction: column;
}
.box{
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: #ffffff;
}
.title{
  width: 97.5%;
  padding-left: 2.5%;
  font-size: 25rpx;
  line-height: 50rpx;
  background-color: #f1f0f0;
}
.list{
  width: 100%;
}
.item{
  width: 95%;
  margin-left: 2.5%;
  border-bottom: 2rpx solid #f1f0f0;
  font-size: 25rpx;
  line-height: 80rpx;
  text-align: left;
}